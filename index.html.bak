document.addEventListener("DOMContentLoaded", function() {
    const button = document.querySelector(".button");
    let isButtonDisabled = false;
    let countdownInterval;

    button.addEventListener("click", function() {
        if (isButtonDisabled) {
            return; // Evita cliques adicionais enquanto o botão estiver desativado
        }

        // Limpa o intervalo atual, se existir
        clearInterval(countdownInterval);

        isButtonDisabled = true;

        let secondsLeft = 60;
        updateButtonText(secondsLeft);

        // Atualiza o texto do botão a cada segundo
        countdownInterval = setInterval(function() {
            secondsLeft--;
            updateButtonText(secondsLeft);

            if (secondsLeft <= 0) {
                isButtonDisabled = false;
                clearInterval(countdownInterval);
                button.classList.remove("opacity-70");
                button.innerText = "Calcular probabilidades"; // Restaura o texto original
            }
        }, 1000);

        const cells = document.querySelectorAll(".mine-block-trevor");
        cells.forEach(function(cell) {
            cell.innerHTML = ''; // Remove as estrelas existentes
        });

        const shuffledCells = Array.from(cells).sort(() => Math.random() - 0.5);

        const starsCount = Math.floor(Math.random() * 7) + 1;

        for (let i = 0; i < starsCount && i < shuffledCells.length; i++) {
            shuffledCells[i].innerHTML = '<img class="w-5 md:w-9" src="/assets/star.png" alt="Icone">';
        }

        button.classList.add("opacity-70");
    });

    function updateButtonText(seconds) {
        button.innerText = `Aguarde (${seconds}s)`;
    }
});
</script>
